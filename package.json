{
  "name": "neutron",
  "productName": "Neutron",
  "version": "0.0.1",
  "description": "Electron base",
  "main": "main.js",
  "scripts": {
    "start": "NODE_ENV=development electron .",
    "build": "npm run build-dll && npm run build-dev",
    "build-dev": "NODE_ENV=development webpack --config ./configs/webpack.config.renderer.dev.babel.js",
    "build-dll": "NODE_ENV=development webpack --config ./configs/webpack.config.renderer.dev.dll.babel.js",
    "dev": "START_HOT=1 npm run start-renderer-dev",
    "start-main-dev": "HOT=1 NODE_ENV=development electron .",
    "start-renderer-dev": "NODE_ENV=development webpack-dev-server --config configs/webpack.config.renderer.dev.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dewyze/neutron"
  },
  "author": {
    "name": "Electron React Boilerplte Maintainers",
    "email": "neutron@dewyze.io"
  },
  "devEngines": {
    "node": ">=10.15.3",
    "npm": ">=5.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "electron": "^5.0.2",
    "electron-devtools-installer": "^2.2.4",
    "install": "^0.12.2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "concurrently": "^4.1.0",
    "devtron": "^1.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
